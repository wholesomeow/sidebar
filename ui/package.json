{
  "name": "ui",
  "version": "1.0.0",
  "main": "main.cjs",
  "scripts": {
    "start": "npx electron .",
    "dev": "vite",
    "build": "vite build",
    "package": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "wait-on": "^8.0.5"
  },
  "build": {
    "appId": "com.sidebar.app",
    "productName": "Sidebar",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "**/*",
      "!backend/**"
    ],
    "extraResources": [
      {
        "from": "backend/",
        "to": "backend/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "electron-is-dev": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
